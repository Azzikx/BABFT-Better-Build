BabftBetterBuild.ResetOnSpawn = false

local Plr = game.Players.LocalPlayer
local PlrGui : PlayerGui = Plr:FindFirstChild("PlayerGui")
local Mouse = Plr:GetMouse()

local BlocksFolder = workspace.Blocks:FindFirstChild(Plr.Name)
local Blocks = {}

local WhiteCol = Color3.new(255, 255, 255)
local TrueCol = Color3.new(0.262745, 1, 0.435294)
local FalseCol = Color3.new(1, 0.113725, 0.129412)
local CurrentOpen = Select 

function Open(FTO)
	CurrentOpen.Visible = false
	CurrentOpen = FTO

	FTO.Visible = true
	Back.Visible = true
	Back.MouseButton1Click:Once(function()
		Open(Select)
	end)
end
function HideMainF(boolean)
	if boolean then
		ShowMain.Position = Main.Position
	else
		Main.Position = ShowMain.Position
	end
	Main.Visible = not boolean
	ShowMain.Visible = boolean
end
HideMain.MouseButton1Click:Connect(function() HideMainF(true) end)
ShowMain.MouseButton1Click:Connect(function() HideMainF(false) end)

MoveMain.MouseButton1Down:Connect(function()
	local deb = true
	MoveMain.MouseButton1Up:Once(function() deb = false end)
	repeat
		ShowMain.Position = UDim2.fromOffset(Mouse.X - 20 , Mouse.Y - 60)
		task.wait()
	until deb == false
end)

for i, v in pairs(List:GetChildren()) do
	if v:IsA("TextButton") then
		local FrameToOpen = Main:WaitForChild(string.sub(v.Name, 5, string.len(v.Name)))
		v.MouseButton1Click:Connect(function()
			Open(FrameToOpen)
		end)
	end
end
Main.InfoButton.MouseButton1Click:Connect(function() Open(Main.Info) end)
local function CorrectNum(Num, Max)
	local Num = tonumber(Num)
	if Num then
		math.clamp(Num, 0, Max)
		return Num
	else 
		return Max
	end
end
----------------------------==R-T==--------------------------
local RTOptions = {
	Enabled = false,
	R = 255, G = 255, B = 255,
	Transparency = 0
}

for i, v : TextBox in pairs(RGBRT:GetChildren()) do
	local Color = string.sub(v.Name, 1, 1)
	v.Changed:Connect(function()
		RTOptions[Color] = CorrectNum(v.Text, 255)
	end)
end
RT.TransparencyRT.Changed:Connect(function()
	RTOptions.Transparency = CorrectNum(RT.TransparencyRT.Text, 100)
end)
EnableRT.MouseButton1Click:Connect(function()
	RTOptions.Enabled = not RTOptions.Enabled
	if RTOptions.Enabled then
		EnableRT.BackgroundColor3 = TrueCol
	else
		EnableRT.BackgroundColor3 = FalseCol
	end
end)

BlocksFolder.ChildAdded:Connect(function (obj)
	obj.ChildAdded:Connect(function (Box)
		if string.find(Box.Name, "SB") and RTOptions.Enabled then
			Box.LineThickness = 0.02
			Box.Color3 = Color3.fromRGB(RTOptions.R, RTOptions.G, RTOptions.B)
			Box.Transparency = RTOptions.Transparency / 100
		end
	end)
end)
workspace.ChildAdded:Connect(function(obj)
	if obj.Name == "TrowelBoundingBox" and RTOptions.Enabled then
		local SB = obj:WaitForChild("SelectionBox")
		SB.Transparency = 0.6
	end
end)
-------------== FP ==-----------------
local BlocksFolder = workspace.Blocks:WaitForChild(Plr.Name)

local Parametrs = {"True Only", "False Only", "True and False"}
local CurrentPar = 1
local FindType = "None"
FPParametr.MouseButton1Click:Connect(function()
	CurrentPar += 1
	if Parametrs[CurrentPar] == nil then
		CurrentPar = 1
	end
	FPParametr.Text = Parametrs[CurrentPar]
end)

local function SelectType(Button : TextButton)
	FPAnchored.BackgroundColor3 = WhiteCol
	FPCollide.BackgroundColor3 = WhiteCol
	FPNone.BackgroundColor3 = WhiteCol
	
	Button.BackgroundColor3 = TrueCol
end
FPAnchored.MouseButton1Click:Connect(function() FindType = "Anchored"; SelectType(FPAnchored) end)
FPCollide.MouseButton1Click:Connect(function() FindType = "CanCollide"; SelectType(FPCollide) end)
FPNone.MouseButton1Click:Connect(function() FindType = "None"; SelectType(FPNone) end)

local HLFolder = Instance.new("Folder", FP)
local HLFind = Instance.new("SelectionBox")
HLFind.SurfaceTransparency = 0.3
HLFind.Transparency = 1

local function ShowBlocks()
	for i, v in pairs(BlocksFolder:GetChildren()) do
		local Trans = v:GetAttribute("StartT")
		if Trans ~= nil and Trans ~= "None" then
			v.PPart.Transparency = Trans
			v:SetAttribute("StartT", "None")
		end
	end
	--[[local Teams = workspace:FindFirstChild("Teams")
	if not Teams then return end
	for i, v in pairs(Teams:FindFirstChild(tostring(Plr.TeamColor).."Team"):FindFirstChild("ZoneCoverParts"):GetChildren()) do
		v.Transparency = 0	
	end]]
end
local function HideBlocks()
	for i, v in pairs(BlocksFolder:GetChildren()) do
		if v:GetAttribute("StartT") == nil or v:GetAttribute("StartT") == "None" then
			v:SetAttribute("StartT", v.PPart.Transparency)
		end
		v.PPart.Transparency = 0.2
	end
	--[[local Teams = workspace:FindFirstChild("Teams")
	if not Teams then return end
	for i, v in pairs(Teams:FindFirstChild(tostring(Plr.TeamColor).."Team"):FindFirstChild("ZoneCoverParts"):GetChildren()) do
		v.Transparency = 0.15
	end]]
end

FPButton.MouseButton1Click:Connect(function()
	HLFolder:ClearAllChildren()
	if FindType == "None" then 
		ShowBlocks()
		return
	end
	HideBlocks()
	for i, v in pairs(BlocksFolder:GetChildren()) do
		if CurrentPar == 1 or CurrentPar == 3 then
			if v.PPart[FindType] == true then
				local HL = HLFind:Clone()
				HL.SurfaceColor3 = TrueCol
				HL.Adornee = v.PPart
				HL.Parent = HLFolder
			end
		end
		if CurrentPar == 2 or CurrentPar == 3 then
			if v.PPart[FindType] == false then
				local HL = HLFind:Clone()
				HL.SurfaceColor3 = FalseCol
				HL.Adornee = v.PPart
				HL.Parent = HLFolder
			end
		end
	end
end)
--------==RSV==-----
local RSVOptions = {
	Enabled = false,
	R = 255, G = 255, B = 255,
	Transparency = 80, Size = 10
}

for i, v : TextBox in pairs(RGBRSV:GetChildren()) do
	local Color = string.sub(v.Name, 1, 1)
	v.Changed:Connect(function()
		RSVOptions[Color] = CorrectNum(v.Text, 255)
	end)
end
TransparencyRSV.Changed:Connect(function()
	RSVOptions.Transparency = CorrectNum(TransparencyRSV.Text, 100)
end)
SizeRSV.Changed:Connect(function()
	RSVOptions.Size = CorrectNum(SizeRSV.Text, 1000)
end)
EnableRSV.MouseButton1Click:Connect(function()
	RSVOptions.Enabled = not RSVOptions.Enabled
	if RSVOptions.Enabled then
		EnableRSV.BackgroundColor3 = TrueCol
	else
		EnableRSV.BackgroundColor3 = FalseCol
	end
end)
local TempParts = Instance.new("Folder", workspace)
TempParts.Name = "TempParts"
local EAxis = Enum.Axis

local GBB = Instance.new("BillboardGui")
GBB.Size = UDim2.fromOffset(100, 50)
GBB.AlwaysOnTop = true
local Gradus = Instance.new("TextLabel", GBB)
Gradus.Size = UDim2.fromScale(1, 1)
Gradus.BackgroundTransparency = 1
Gradus.TextScaled = true
Gradus.TextStrokeTransparency = 0
Gradus.TextStrokeColor3 = Color3.new(1, 1, 1)

local function CeilVector(Vector : Vector3)
	return Vector3.new(math.round(Vector.X), math.round(Vector.Y), math.round(Vector.Z))
end

local CurrentHandle = nil
PlrGui.ChildAdded:Connect(function(obj)
	if not obj:IsA("ArcHandles") or not RSVOptions.Enabled or obj == CurrentHandle then return end
	local RotHandle : ArcHandles = obj
	CurrentHandle = obj
	
	local d = RotHandle.MouseButton1Up:Connect(function() GBB.Parent = script; ShowBlocks(); TempParts:ClearAllChildren() end)
	local c = RotHandle.MouseButton1Down:Connect(function(Axis)
		GBB.Parent = script
		TempParts:ClearAllChildren()
		--local Scale = 2
		local Scale = tonumber(PlrGui.TrowelToolDisplayGui.ModeImage.More.Scale.TextBox.Text)
		if Scale < 1 or Scale > 360 or Scale ~= math.round(Scale) or 360 % Scale ~= 0 then return end
		HideBlocks()
		local StepPart = Instance.new("Part")
		StepPart.Color = Color3.fromRGB(RSVOptions.R, RSVOptions.G, RSVOptions.B)
		StepPart.Transparency = RSVOptions.Transparency / 100
		StepPart.Anchored = true
		StepPart.CanCollide = false
		StepPart.CanQuery = false
		local TBB = game.Workspace:WaitForChild("TrowelBoundingBox")
		
		StepPart.Orientation = TBB.Orientation
		StepPart.Position = TBB.Position
		StepPart.Size = Vector3.new(0.05, RSVOptions.Size, 0.05)
		
		local Step = 0
		if Axis.Name == "Y" then
			StepPart.Size = Vector3.new(RSVOptions.Size, 0.05, 0.05)
			StepPart.Position = TBB.Position
			Step = Vector3.new(0, Scale, 0)
		elseif Axis.Name == "X" then
			Step = Vector3.new(Scale, 0, 0)
		elseif Axis.Name == "Z" then
			Step = Vector3.new(0, 0, Scale)
		end
		for i = 0, (360 / Scale) - 1 do
			local StepClone = StepPart:Clone()
			local NewStep = Step * i
			StepClone.CFrame *= CFrame.Angles(math.rad(NewStep.X), math.rad(NewStep.Y), math.rad(NewStep.Z))
			
			if i % 2 == 0 then
				if Axis.Name ~= "Y" then
					StepClone.Size -= Vector3.new(0, RSVOptions.Size / 15,0)
				else
					StepClone.Size -= Vector3.new(RSVOptions.Size / 15,0,0)
				end
			end
			
			local AcSize
			if Axis.Name ~= "Y" then
				AcSize = CFrame.new(0, StepClone.Size.Y / 2, 0)
			else
				AcSize = CFrame.new(StepClone.Size.X / 2, 0, 0)
			end
			StepClone.CFrame *= AcSize
			
			if i == 0 then
				StepClone.BrickColor = BrickColor.Red()
				StepClone.Material = "Neon"
			end
			StepClone.Parent = TempParts
			
			local f = TBB.Changed:Connect(function(prop)
				if CeilVector(TBB.Orientation) == CeilVector(StepClone.Orientation) then
					GBB.Parent = StepClone
					Gradus.Text = tostring(Step[Axis.Name] * i) .. "°"
					StepClone.Material = "Neon"
				else
					StepClone.Material = "Plastic"
				end
			end)
			StepClone.Destroying:Once(function() f:Disconnect() end)
		end
	end)
	RotHandle.Destroying:Connect(function()
		GBB.Parent = script
		TempParts:ClearAllChildren()
		ShowBlocks()
		d:Disconnect()
		c:Disconnect()
	end)
end)
--
BabftBetterBuild.Destroying:Connect(function()
	script:Destroy()
end)
